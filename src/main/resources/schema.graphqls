# This will also resolve to string in Decimal format
#scalar Decimal
type Account{
    id: ID!
    name: String!
    description: String!,
    positions:[Position]!
}

#This represents a single banking transactions
type Transaction{
    id: ID!
    title: String!
    description: String!
    value: String!
}

type Position{
    id: ID!
    ticker: String!
    shares: String!
    price: String!
    date:String!
}

type Query{
    accounts:[Account]!
}